{
  "name": "express-dustjs",
  "version": "1.0.0",
  "description": "DustJS middleware for ExpressJS",
  "main": "index.js",
  "scripts": {
    "gulp": "node ./node_modules/gulp/bin/gulp.js",
    "mocha": "node --harmony node_modules/mocha/bin/_mocha --reporter nyan --bail --check-leaks test/*.js",
    "mocha:cov": "node --harmony node_modules/istanbul/lib/cli.js cover node_modules/mocha/bin/_mocha -- --reporter spec --check-leaks test/*.js",
    "test": "npm run gulp && npm run mocha",
    "test-travis": "npm run gulp && npm run mocha:cov"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/chrisyip/express-dustjs.git"
  },
  "keywords": [
    "express",
    "dust",
    "dustjs",
    "dust.js",
    "view",
    "template"
  ],
  "author": "Chris Yip <i@chrisyip.im>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/chrisyip/express-dustjs/issues"
  },
  "homepage": "https://github.com/chrisyip/express-dustjs",
  "dependencies": {
    "dustjs-helpers": "1.5.0",
    "dustjs-linkedin": "2.5.1",
    "lodash": "^2.4.1"
  },
  "devDependencies": {
    "bluebird": "^2.8.1",
    "express": "^4.11.0",
    "gulp": "^3.8.10",
    "gulp-jshint": "^1.9.0",
    "istanbul": "^0.3.5",
    "jshint": "^2.5.11",
    "jshint-stylish": "^1.0.0",
    "mocha": "^2.1.0",
    "request": "^2.51.0"
  }
}
